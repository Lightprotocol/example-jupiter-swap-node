{
    "compilerOptions": {
      "target": "ES2022",
      "module": "NodeNext",
      "moduleResolution": "node16",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "allowImportingTsExtensions": true,
      "skipLibCheck": false,
      "noEmit": true
    },
    "include": ["src/**/*"]
  }