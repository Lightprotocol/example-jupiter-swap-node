{
  "name": "jup-swap",
  "version": "1.0.0",
  "main": "index.js",
  "author": "light protocol maintainers",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@lightprotocol/compressed-token": "~0.16.0",
    "@lightprotocol/jup-api-adapter": "~0.1.3",
    "@lightprotocol/stateless.js": "~0.16.0",
    "@solana/spl-token": "0.4.8",
    "@solana/web3.js": "1.95.3",
    "@types/node": "^22.9.2",
    "bs58": "^6.0.0",
    "dotenv": "^16.4.5",
    "promise-retry": "^2.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.6",
    "@types/promise-retry": "^1.1.6",
    "fastestsmallesttextencoderdecoder": "^1.0.22",
    "prettier": "^3.3.3",
    "tsx": "^4.7.1",
    "yarn": "^1.22.22"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--no-warnings --no-deprecation' tsx watch src/index.ts",
    "quote": "tsx src/index.ts quote true",
    "quote-skip-logs": "tsx src/index.ts quote false",
    "swap": "tsx src/index.ts swap true",
    "debug": "NODE_DEBUG=* node --trace-warnings --trace-deprecation --trace-uncaught node_modules/.bin/tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\""
  }
}
